{
  "name": "pirate-midi-usb",
  "version": "0.0.0-development",
  "description": "Easily interact with Pirate Midi devices over USB from Node.js",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "examples:intro": "wds --watch --supervise examples/intro.ts",
    "examples:backup": "wds --watch --supervise examples/backup.ts",
    "build": "tsc -p ./tsconfig.build.json",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "postinstall:dev": "wds --watch --supervise ./scripts/download-device-descriptors.ts",
    "postinstall": "wds ./scripts/download-device-descriptors.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GriffinSauce/pirate-midi-usb.git"
  },
  "license": "MIT",
  "author": {
    "name": "Joris Griffioen",
    "email": "GriffinSauce@users.noreply.github.com",
    "url": "https://github.com/GriffinSauce"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "boilerplate",
    "template",
    "typescript",
    "vscode",
    "jest",
    "husky",
    "commitizen",
    "semantic-release",
    "codecov"
  ],
  "bugs": {
    "url": "https://github.com/GriffinSauce/pirate-midi-usb/issues"
  },
  "homepage": "https://github.com/GriffinSauce/pirate-midi-usb#readme",
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@serialport/binding-mock": "^10.2.2",
    "@serialport/stream": "^10.3.0",
    "@swc/core": "^1.2.210",
    "@swc/jest": "^0.2.21",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.4",
    "@types/node": "^12.20.11",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "codecov": "^3.8.3",
    "esbuild": "^0.14.48",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^28.1.2",
    "lint-staged": "^10.5.4",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.2",
    "typescript": "^4.2.4",
    "wds": "^0.12.0",
    "xstate": "^4.32.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@serialport/parser-regex": "^10.3.0",
    "debug": "^4.3.4",
    "serialport": "^10.4.0",
    "suretype": "^3.0.5"
  }
}
